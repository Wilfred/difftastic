{
  "name": "tree-sitter-godot-resource",
  "version": "0.3.1",
  "description": "tree-sitter parser for godot text resources (tres, tscn)",
  "main": "bindings/node",
  "keywords": [
    "lexer",
    "parser",
    "tres",
    "tscn"
  ],
  "author": "Preston Knopp",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.2",
    "prebuild-install": "^6.0.0"
  },
  "devDependencies": {
    "prebuild": "^10.0.1",
    "tree-sitter-cli": "0.20.0"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "prebuild": "prebuild -r electron --strip --verbose -t 7.0.0 -t 8.0.0 -t 9.0.0 -t 10.0.0 -t 11.0.0",
    "prebuild:upload": "prebuild --upload-all",
    "generate": "tree-sitter generate",
    "test": "tree-sitter test"
  },
  "repository": "https://github.com/PrestonKnopp/tree-sitter-godot-resource.git"
}
