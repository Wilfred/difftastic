{
  "name": "tree-sitter-rust",
  "version": "0.13.0",
  "description": "Rust grammar for tree-sitter",
  "main": "index.js",
  "keywords": [
    "parser",
    "rust"
  ],
  "author": "Maxim Sokolov <maxim0xff@gmail.com> (https://github.com/MaximSokolov)",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.8.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.13.5",
    "eslint": "^3.19.0"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "test": "npm run lint && tree-sitter test && npm run parse-examples",
    "parse-examples": "tree-sitter parse $(find examples -name '*.rs') --quiet --time",
    "lint": "eslint ./grammar.js"
  }
}
