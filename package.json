{
  "name": "tree-sitter-rust",
  "version": "0.13.7",
  "description": "Rust grammar for tree-sitter",
  "main": "index.js",
  "keywords": [
    "parser",
    "rust"
  ],
  "author": "Maxim Sokolov <maxim0xff@gmail.com> (https://github.com/MaximSokolov)",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.8.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.15.3",
    "eslint": "^3.19.0"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "test": "eslint ./grammar.js && tree-sitter test && script/parse-examples",
    "test-windows": "eslint ./grammar.js && tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.rs",
      "name": "Rust",
      "highlights": "src/highlights.json",
      "file-types": [
        "rs"
      ]
    }
  ]
}
