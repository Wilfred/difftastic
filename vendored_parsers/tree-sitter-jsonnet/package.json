{
  "name": "tree-sitter-jsonnet",
  "version": "0.0.1",
  "description": "jsonnet grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.6"
  },
  "scripts": {
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.jsonnet",
      "file-types": [
        "jsonnet"
      ],
      "injection-regex": "jsonnet"
    }
  ]
}
