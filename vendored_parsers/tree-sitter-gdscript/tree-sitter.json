    "license": "MIT",
    "description": "Grammar for Godot's built-in scripting language.",
    "authors": [
      {
        "name": "Preston Knopp",
        "email": "prestonknopp@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/PrestonKnopp/tree-sitter-gdscript"
    }
  },
  "bindings": {
